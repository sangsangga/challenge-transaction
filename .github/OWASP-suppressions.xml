<?xml version="1.0" encoding="UTF-8"?>
<suppressions xmlns="https://jeremylong.github.io/DependencyCheck/dependency-suppression.1.3.xsd">
    
    <!-- Suppress false positives for Spring Boot dependencies -->
    <suppress>
        <notes><![CDATA[
        Spring Boot Actuator - False positive for CVE in older versions
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/org\.springframework\.boot/spring-boot-actuator.*$</packageUrl>
        <cve>CVE-2021-22060</cve>
    </suppress>
    
    <!-- Suppress known issues that don't affect our usage -->
    <suppress>
        <notes><![CDATA[
        PostgreSQL JDBC driver - DoS vulnerability not applicable in our usage
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/org\.postgresql/postgresql.*$</packageUrl>
        <cve>CVE-2022-31197</cve>
    </suppress>
    
    <!-- Suppress test-only dependencies -->
    <suppress>
        <notes><![CDATA[
        Testcontainers - Only used in test scope, not in production
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/org\.testcontainers/.*$</packageUrl>
        <cve>.*</cve>
    </suppress>
    
    <!-- Add more suppressions as needed -->
